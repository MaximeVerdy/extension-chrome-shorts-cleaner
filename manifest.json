{
  "manifest_version": 3,
  "name": "Shorts Cleaner",
  "version": "1.0.1",
  "description": "Remove all the Shorts videos from the Youtube thread",
  "author": "Maxime Verdy",
  "icons": {
    "128": "./images/128.png",
    "48": "./images/48.png",
    "32": "./images/32.png",
    "16": "./images/16.png"
},
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "contentScript.js"
      ],
      "run_at": "document_end"
    }
  ]
}